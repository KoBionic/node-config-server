{
  "name": "@kobionic/node-config-server",
  "version": "1.2.0-SNAPSHOT",
  "private": false,
  "description": "Cloud Configuration Server using a RESTful API serving either entire files or their parsed content.",
  "keywords": [
    "cloud",
    "configuration",
    "docker",
    "javascript",
    "micro-services",
    "node",
    "rest",
    "server",
    "typescript"
  ],
  "homepage": "https://github.com/KoBionic/node-config-server",
  "bugs": {
    "url": "https://github.com/KoBionic/node-config-server/issues"
  },
  "license": "Apache-2.0",
  "author": "Jeremie Rodriguez <contact@jeremierodriguez.com> (https://github.com/jeremiergz)",
  "files": [
    "dist/"
  ],
  "main": "dist/bin/www.js",
  "bin": {
    "ncs": "dist/bin/www.js",
    "node-config-server": "dist/bin/www.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KoBionic/node-config-server.git"
  },
  "scripts": {
    "build": "gulp build",
    "build:w": "gulp build && gulp watch",
    "clean": "gulp clean",
    "lint": "gulp lint",
    "resources": "gulp resources",
    "start": "node dist/bin/www.js",
    "start:w": "gulp serve",
    "test": "NODE_ENV=test LOG_LEVEL=none jest --coverage --verbose --forceExit",
    "test:cc": "jest --clearCache",
    "version": "echo $npm_package_version"
  },
  "dependencies": {
    "body-parser": "1.18.2",
    "eureka-js-client": "4.4.0",
    "express": "4.16.2",
    "inversify": "4.9.0",
    "js-yaml": "3.10.0",
    "moment": "2.20.1",
    "morgan": "1.9.0",
    "reflect-metadata": "0.1.12",
    "serve-favicon": "2.4.5",
    "winston": "2.4.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.16.8",
    "@types/eureka-js-client": "4.3.1",
    "@types/express": "4.11.0",
    "@types/jest": "22.1.0",
    "@types/js-yaml": "3.10.1",
    "@types/morgan": "1.7.35",
    "@types/node": "9.3.0",
    "@types/serve-favicon": "2.2.30",
    "@types/sinon": "4.1.3",
    "@types/supertest": "2.0.4",
    "@types/winston": "2.3.7",
    "del": "3.0.0",
    "gulp": "3.9.1",
    "gulp-header": "2.0.1",
    "gulp-nodemon": "2.2.1",
    "gulp-sourcemaps": "2.6.3",
    "gulp-tslint": "8.1.2",
    "gulp-typescript": "3.2.4",
    "jest": "22.1.4",
    "run-sequence": "2.2.1",
    "sinon": "4.2.1",
    "supertest": "3.0.0",
    "ts-jest": "22.0.1",
    "ts-node": "4.1.0",
    "tslint": "5.9.1",
    "typescript": "2.6.2",
    "vinyl-buffer": "1.0.1"
  },
  "engines": {
    "node": ">=8"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "mapCoverage": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testEnvironment": "node",
    "testRegex": ".+.spec.ts$",
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    }
  }
}
