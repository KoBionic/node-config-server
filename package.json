{
  "name": "@kobionic/node-config-server",
  "version": "1.6.0-SNAPSHOT",
  "private": false,
  "description": "Centralized configuration server providing a dynamic RESTful API, allowing retrieval of entire files content or their parsed properties.",
  "keywords": [
    "cloud",
    "configuration",
    "docker",
    "javascript",
    "micro-services",
    "node",
    "rest",
    "server",
    "typescript"
  ],
  "homepage": "https://github.com/KoBionic/node-config-server",
  "bugs": {
    "url": "https://github.com/KoBionic/node-config-server/issues"
  },
  "license": "Apache-2.0",
  "author": "Jeremie Rodriguez <contact@jeremierodriguez.com> (https://github.com/jeremiergz)",
  "files": [
    "dist/"
  ],
  "main": "dist/server.js",
  "bin": {
    "ncs": "dist/server.js",
    "node-config-server": "dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KoBionic/node-config-server.git"
  },
  "scripts": {
    "build": "webpack --progress",
    "build:prod": "NODE_ENV=production webpack --progress",
    "start": "node dist/server.js",
    "start:w": "webpack --progress --watch",
    "test": "NODE_ENV=test LOG_WEBSOCKET=false LOG_DIR=logs LOG_LEVEL=none jest --coverage --verbose --forceExit",
    "test:cc": "jest --clearCache",
    "version": "echo $npm_package_version"
  },
  "dependencies": {
    "body-parser": "1.18.2",
    "cors": "2.8.4",
    "eureka-js-client": "4.4.1",
    "express": "4.16.3",
    "helmet": "3.12.0",
    "ini": "1.3.5",
    "js-yaml": "3.11.0",
    "moment": "2.22.0",
    "morgan": "1.9.0",
    "properties": "1.2.1",
    "socket.io": "2.1.0",
    "uuid": "3.2.1",
    "winston": "2.4.1",
    "xml2js": "0.4.19"
  },
  "devDependencies": {
    "@types/body-parser": "1.16.8",
    "@types/cors": "2.8.3",
    "@types/eureka-js-client": "4.3.1",
    "@types/express": "4.11.1",
    "@types/helmet": "0.0.37",
    "@types/ini": "1.3.29",
    "@types/jest": "22.2.2",
    "@types/js-yaml": "3.11.1",
    "@types/morgan": "1.7.35",
    "@types/node": "9.6.4",
    "@types/sinon": "4.3.1",
    "@types/socket.io": "1.4.33",
    "@types/supertest": "2.0.4",
    "@types/uuid": "3.4.3",
    "@types/webpack": "4.1.3",
    "@types/winston": "2.3.9",
    "@types/xml2js": "0.4.2",
    "clean-webpack-plugin": "0.1.19",
    "copy-webpack-plugin": "4.5.1",
    "jest": "22.4.3",
    "nodemon-webpack-plugin": "4.0.2",
    "sinon": "4.5.0",
    "supertest": "3.0.0",
    "ts-jest": "22.4.2",
    "ts-loader": "4.2.0",
    "ts-node": "5.0.1",
    "tslint": "5.9.1",
    "typescript": "2.8.1",
    "webpack": "4.5.0",
    "webpack-cli": "2.0.14",
    "webpack-permissions-plugin": "1.0.0"
  },
  "engines": {
    "node": ">=8"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testEnvironment": "node",
    "testRegex": ".+.spec.ts$",
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    }
  }
}
